{
  "name": "rmm-ocm-component-library",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "",
  "scripts": {
    "build:sb": "rm -rf docs && build-storybook -o docs -s src/assets",
    "build:pack": "yarn pack --filename rmm-ocm-package.tgz; tar -xzf rmm-ocm-package.tgz; rm rmm-ocm-package.tgz",
    "build:test": "yarn build; yarn build:package",
    "build0": "yarn test && node esbuild.js",
    "build": "tsc --emitDeclarationOnly && node esbuild.js",
    "clean": "rm -rf dist; rm -rf package",
    "lint:fix": "eslint \"src/*\" --cache --fix",
    "lint": "eslint \"src/*\"",
    "new:common": "hygen lib common && npx prettier -c ./src/common/index.ts -w",
    "new:component": "hygen lib component && npx prettier -c ./src/components/index.ts -w",
    "new:hook": "hygen lib hook && npx prettier -c ./src/hooks/index.ts -w",
    "prebuild": "yarn clean",
    "prepare": "husky install",
    "start": "NODE_ENV=development start-storybook -p 6006",
    "start:served": "NODE_ENV=development start-storybook -p 6006 -s ./src/assets",
    "test": "yarn lint && yarn types && jest",
    "types": "tsc --noEmit",
    "preversion": "yarn test && yarn build",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-docs": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "auto-changelog": "^2.3.0",
    "babel-loader": "^8.2.2",
    "esbuild": "^0.12.24",
    "esbuild-plugin-svgr": "^0.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-mdx": "^1.15.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.1",
    "fs-extra": "^10.0.0",
    "husky": ">=6",
    "jest": "^27.1.0",
    "lint-staged": ">=10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook-css-modules-preset": "^1.1.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint:fix",
      "jest --passWithNoTests"
    ]
  },
  "auto-changelog": {
    "commitLimit": false,
    "template": "keepachangelog"
  },
  "dependencies": {
    "@types/classnames": "^2.3.1",
    "classnames": "^2.3.1",
    "css-tree": "^1.1.3",
    "esbuild-css-modules-plugin": "^2.0.9",
    "polished": "^4.1.3",
    "react-inlinesvg": "^2.3.0"
  }
}
